{
  "name": "typescript-nodejs-eslint-prettier-starter",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "main": "src/main.js",
  "scripts": {
    "build": "tsc",
    "check": "tsc --noEmit && eslint . && npx prettier --check .",
    "lint": "eslint . --fix && npx prettier --write .",
    "start": "npm run build && node build/main.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@types/node": "^16.18.96",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^7.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "3.2.5",
    "typescript": "^5.4.3"
  },
  "engines": {
    "node": ">=16.16.0"
  }
}
